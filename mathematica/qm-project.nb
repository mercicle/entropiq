 CellChangeTimes->{{3.8195399742886457`*^9, 
  3.819540001439283*^9}},ExpressionUUID->"b17054e1-560f-469f-bb6d-\
172c980fe6d0"],

Cell[BoxData[""], "Input",
 CellFrame->{{0, 0}, {0, 2}},
 CellChangeTimes->{
  3.819540007113771*^9, {3.819540227097126*^9, 
   3.819540243358179*^9}},ExpressionUUID->"de842ddb-025c-4137-ac87-\
c36fc851840f"],

Cell[TextData[{
 "Show that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox["x", "n"], ",", "p"}], "]"}], "=", 
    RowBox[{"i", " ", "\[HBar]", " ", "n", " ", 
     SuperscriptBox["x", 
      RowBox[{"n", "-", "1"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2541d929-a2a0-4f25-93bc-1e10523783c1"]
}], "Text",
 CellChangeTimes->{{3.821648421847875*^9, 
  3.821648475681102*^9}},ExpressionUUID->"b683bbb6-919e-4e1b-8677-\
6a0f1bdb3394"],

Cell[TextData[{
 "Using the method of section 2.3, we use a \[OpenCurlyDoubleQuote]test \
function\[CloseCurlyDoubleQuote] f(x) to act on and then throw it away at the \
end.\n\nRecall  the momentum operator ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "=", 
    RowBox[{
     FractionBox["\[HBar]", "i"], 
     FractionBox[
      RowBox[{"d", " "}], "dx"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2ee5f68f-6dc0-4727-8b15-7eaf1543acd6"],
 ".\n\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{"[", 
     RowBox[{
      SuperscriptBox["x", "n"], ",", "p"}], "]"}], "=", " ", 
    RowBox[{
     RowBox[{
      SuperscriptBox["x", "n"], "p"}], " ", "-", " ", 
     RowBox[{"p", " ", 
      SuperscriptBox["x", "n"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"                ", 
    RowBox[{"=", "  ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["x", "n"], 
       FractionBox["\[HBar]", "i"], 
       FractionBox[
        RowBox[{"d", " "}], "dx"]}], " ", "-", " ", 
      RowBox[{
       FractionBox["\[HBar]", "i"], 
       FractionBox[
        RowBox[{"d", " "}], "dx"], " ", 
       SuperscriptBox["x", "n"]}]}]}]}], TraditionalForm]}],ExpressionUUID->
  "7494bd6f-69b8-4dc6-bfee-a25b82bd366b"],
 "\n\nNow introducing the test function f(x):\n\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       SuperscriptBox["x", "n"], ",", "p"}], "]"}], " ", 
     RowBox[{"f", "(", "x", ")"}]}], " ", "=", "  ", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        SuperscriptBox["x", "n"], 
        FractionBox["\[HBar]", "i"], 
        FractionBox[
         RowBox[{"d", " "}], "dx"]}], " ", "-", " ", 
       RowBox[{
        FractionBox["\[HBar]", "i"], 
        FractionBox[
         RowBox[{"d", " "}], "dx"], " ", 
        SuperscriptBox["x", "n"]}]}], "]"}], " ", 
     RowBox[{"f", "(", "x", ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"                           ", 
    RowBox[{"=", "  ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["x", "n"], 
       FractionBox["\[HBar]", "i"], 
       FractionBox[
        RowBox[{"d", " "}], "dx"], 
       RowBox[{"f", "(", "x", ")"}]}], " ", "-", " ", 
      RowBox[{
       FractionBox["\[HBar]", "i"], 
       FractionBox[
        RowBox[{"d", " "}], "dx"], " ", 
       SuperscriptBox["x", "n"], 
       RowBox[{"f", "(", "x", ")"}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t\t", 
    RowBox[{"=", "  ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["x", "n"], 
       FractionBox["\[HBar]", "i"], 
       FractionBox[
        RowBox[{"d", " "}], "dx"], 
       RowBox[{"f", "(", "x", ")"}]}], " ", "-", " ", 
      RowBox[{
       FractionBox["\[HBar]", "i"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SuperscriptBox["nx", 
           RowBox[{"n", "-", "1"}]], " ", 
          RowBox[{"f", "(", "x", ")"}]}], "+", " ", 
         RowBox[{
          SuperscriptBox["x", "n"], 
          FractionBox[
           RowBox[{"d", " "}], "dx"], 
          RowBox[{"f", "(", "x", ")"}]}]}], ")"}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"                          ", 
    RowBox[{"=", "  ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["x", "n"], 
       FractionBox["\[HBar]", "i"], 
       FractionBox[
        RowBox[{"d", " "}], "dx"], 
       RowBox[{"f", "(", "x", ")"}]}], " ", "-", " ", 
      RowBox[{
       FractionBox["\[HBar]", "i"], 
       SuperscriptBox["nx", 
        RowBox[{"n", "-", "1"}]], 
       RowBox[{"f", "(", "x", ")"}]}], " ", "-", " ", 
      RowBox[{
       FractionBox["\[HBar]", "i"], " ", 
       SuperscriptBox["x", "n"], 
       FractionBox[
        RowBox[{"d", " "}], "dx"], 
       RowBox[{"f", "(", "x", ")"}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"                          ", 
    RowBox[{"=", "  ", 
     RowBox[{
      RowBox[{
       RowBox[{"-", " ", 
        FractionBox["\[HBar]", "i"]}], 
       SuperscriptBox["nx", 
        RowBox[{"n", "-", "1"}]], 
       RowBox[{"f", "(", "x", ")"}]}], "  ", "+", 
      RowBox[{"[", 
       RowBox[{
        RowBox[{
         SuperscriptBox["x", "n"], 
         FractionBox["\[HBar]", "i"], 
         FractionBox[
          RowBox[{"d", " "}], "dx"], 
         RowBox[{"f", "(", "x", ")"}]}], "  ", "-", " ", 
        RowBox[{
         SuperscriptBox["x", "n"], 
         FractionBox["\[HBar]", "i"], " ", 
         FractionBox[
          RowBox[{"d", " "}], "dx"], 
