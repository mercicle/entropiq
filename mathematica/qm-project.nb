 Cell[BoxData[
  RowBox[{
   SuperscriptBox["e", 
    RowBox[{
     RowBox[{"-", "2"}], " ", "i", " ", "t", " ", "\[Omega]"}]], " ", 
   SubscriptBox["c", "3"]}]],
  CellChangeTimes->{{3.821891489984045*^9, 3.821891503033197*^9}},
  ExpressionUUID->"a1f3efe4-b743-492a-9fcd-9016851baa53"],
 "    = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    SuperscriptBox["e", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "2"}], " ", "i", " ", "t", " ", "\[Omega]"}], "+", 
      RowBox[{"2", " ", "i", " ", "t", " ", "\[Omega]"}]}]], "|", 
    SubscriptBox["c", "3"], 
