Cell[TextData[{
 "Consider a 3-d vector space spanned by the orthonormal basis ",
 Cell[BoxData[
  TemplateBox[{"1"},
   "Ket"]],ExpressionUUID->"20e47121-20c5-42c5-8ea3-cee5ec9aca5e"],
 ", ",
 Cell[BoxData[
  TemplateBox[{"2"},
   "Ket"]],ExpressionUUID->"601b7ec0-47ee-49fa-ae6b-f9f3b72ddfee"],
 " ",
 Cell[BoxData[
  TemplateBox[{"3"},
   "Ket"]],ExpressionUUID->"4535ae92-70a0-4733-9c9c-a020bac0214f"],
 ". Kets ",
 Cell[BoxData[
  TemplateBox[{"\[Alpha]"},
   "Ket"]],ExpressionUUID->"be1b7c84-c2ae-40f1-b990-da51a18ac9e0"],
 " and ",
 Cell[BoxData[
  TemplateBox[{"\[Beta]"},
   "Ket"]],ExpressionUUID->"15ec4d84-88b8-44e4-a7bf-580d46249b62"],
 "are given by:\n",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{"\[Alpha]"},
    "Ket"], " ", "=", " ", 
   RowBox[{
    RowBox[{"i", " ", 
     TemplateBox[{"1"},
      "Ket"]}], "-", 
    RowBox[{"2", 
     TemplateBox[{"2"},
      "Ket"]}], "-", 
    RowBox[{"i", 
     TemplateBox[{"3"},
      "Ket"]}]}]}]],ExpressionUUID->"8a6b22df-a727-4004-8335-357326a18783"],
 " and \n",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{"\[Beta]"},
    "Ket"], " ", "=", " ", 
   RowBox[{
    RowBox[{"i", " ", 
     TemplateBox[{"1"},
      "Ket"]}], "+", 
    RowBox[{"2", 
     TemplateBox[{"3"},
      "Ket"]}]}]}]],ExpressionUUID->"fe539dd7-761f-47f8-969c-59f6e6c10dc6"]
}], "Text",
