Cell[TextData[{
 "For what range of \[Nu] is the function f(x) = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "\[Nu]"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "1a1ea31c-9f8c-4fa9-951d-6fbfddba8da1"],
 " in Hilbert space, on the interval (0,1). Assume \[Nu] is real but not \
necessarily positive. "
}], "Text",
 CellChangeTimes->{
  3.821648215909912*^9},ExpressionUUID->"b56ad6a2-d941-4c60-9294-\
ec6935e9a217"],

Cell[TextData[{
 "From the definition of a Hilbert space and the property of the inner \
product of f(x) with itself [given by 3.9], we have:\n",
 Cell[BoxData[{
  RowBox[{
   TemplateBox[{"f","f"},
    "BraKet"], "=", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "1"], " ", 
    RowBox[{"|", "f", 
     SuperscriptBox["|", "2"], " ", 
     RowBox[{"\[DifferentialD]", "x"}]}]}]}], "\[IndentingNewLine]", 
  RowBox[{"            ", 
   RowBox[{"=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "1"], " ", 
     RowBox[{
      SuperscriptBox["x", "\[Nu]"], 
      SuperscriptBox["x", "\[Nu]"], " ", 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], "\[IndentingNewLine]", 
  RowBox[{"            ", 
   RowBox[{"=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "1"], " ", 
     RowBox[{
      SuperscriptBox["x", 
       RowBox[{"2", "\[Nu]"}]], " ", 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], "\[IndentingNewLine]", 
  RowBox[{"            ", 
   RowBox[{"=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        FractionBox["1", 
         RowBox[{
          RowBox[{"2", "\[Nu]"}], " ", "+", " ", "1"}]], 
        SuperscriptBox["x", 
         RowBox[{
          RowBox[{"2", "\[Nu]"}], "+", "1"}]]}], ")"}], 
      SubsuperscriptBox["|", "0", "1"]}], "\[IndentingNewLine]", "       ", 
     "=", " ", 
     RowBox[{
      FractionBox["1", 
       RowBox[{
        RowBox[{"2", "\[Nu]"}], " ", "+", " ", "1"}]], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["1", 
         RowBox[{
          RowBox[{"2", "\[Nu]"}], "+", "1"}]], " ", "-", " ", 
        SuperscriptBox["0", 
         RowBox[{
          RowBox[{"2", "\[Nu]"}], "+", "1"}]]}], ")"}]}]}]}]}]}],
  ExpressionUUID->"aaa59779-de51-4764-b626-b10e0990ca1b"]
}], "Text",
 CellChangeTimes->{{3.821794402497964*^9, 3.821794581546815*^9}, {
  3.8217946241967363`*^9, 3.821794656424404*^9}, {3.821794704944818*^9, 
  3.821794732380156*^9}, {3.821794775540064*^9, 3.821794786899467*^9}, {
  3.821795084027541*^9, 
  3.821795091774467*^9}},ExpressionUUID->"7a42eeed-f4fb-4f22-8122-\
6069241c260d"],

Cell[TextData[{
 "So to be in Hilbert space, 2\[Nu] + 1  > 0, or,  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Nu]", ">", " ", 
    RowBox[{"-", 
     FractionBox["1", "2"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "44c54a7d-2ddd-488c-a94e-0fd1494bbe56"],
 "."
}], "Text",
 CellChangeTimes->{{3.821794860296452*^9, 
  3.8217949555080423`*^9}},ExpressionUUID->"48c6b650-d8c2-4228-bc2b-\
f679dc2b9eb6"],

Cell[TextData[{
 StyleBox["For f(x) to be in Hilbert space ",
  Background->RGBColor[1, 0.85, 0.85]],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Nu]", ">", " ", 
    RowBox[{"-", 
     FractionBox["1", "2"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  Background->RGBColor[1, 0.85, 0.85],ExpressionUUID->
  "a9dacba1-b81e-45c7-b856-d9e4f7fbe5df"]
}], "Text",
 CellChangeTimes->{{3.8217950141218843`*^9, 3.821795064118331*^9}},
 TextAlignment->Center,ExpressionUUID->"c391bd1d-0575-4664-a3fe-287d8b73f53d"],

